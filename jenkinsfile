pipeline {
    agent any

    tools {
        maven 'maven'
    }

    stages {
        stage('Pull') {
            steps {
                git url: "https://github.com/re-blank/Jank8-sSampleWebProject"
            }
        stage('Build') {
            steps {
                sh 'mvn package -DskipTests=true'
                sh 'buildah bud .'
                }
            }
        }
    }
}
