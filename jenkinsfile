pipeline {
    agent any

    tools {
        maven 'maven3'
    }

    stages {
        stage('Pull') {
            steps {
                git "https://github.com/re-blank/Jank8-sSampleWebProject"
            }
        }
        stage('Build') {
            steps {
                    sh 'mvn package -DskipTests=true'
                    sh 'buildah bud .'
            }
        }
    }
}
